{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.11.16",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/react-relay": "^14.0.0",
    "@types/relay-runtime": "^14.0.0",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-relay": "^14.0.0",
    "react-scripts": "^5.0.1",
    "relay-runtime": "^14.0.0",
    "typescript": "^4.9.5",
    "web-vitals": "^3.3.1"
  },
  "scripts": {
    "start": "npm run relay && craco start",
    "dev": "npm run relay && craco start",
    "build": "npm run type-check && npm run lint && npm run relay && craco build",
    "test": "craco test",
    "test:ci": "cross-env CI=true craco test --watchAll=false --testPathIgnorePatterns=PaginatedList.test.tsx --passWithNoTests",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:debug": "craco --inspect-brk test --runInBand --no-cache",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "eject": "react-scripts eject",
    "relay": "relay-compiler",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --max-warnings=0",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "autoprefixer": "^10.4.14",
    "babel-plugin-relay": "^14.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.12.0",
    "globby": "^11.0.4",
    "jest-axe": "^7.0.1",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "msw": "^1.2.1",
    "postcss": "^8.4.21",
    "relay-compiler": "^14.0.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.1",
    "ts-jest": "^29.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/__generated__/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "reporters": [
      "default",
      ["jest-junit", {"outputDirectory": "coverage", "outputName": "junit.xml"}]
    ]
  }
}
